/* Check Boxes */

// input[type="checkbox"]{
//     display:none;
// }
input[type="checkbox"] + label span {
    display: inline-block;
    width: 4vh;
    height: 4vh;
    border-radius: 2vh;
    margin-right: 0.5vw;
    background:url(pictures/uncheck1.png);
    background-size:contain;
    cursor:pointer;
}
input[type="checkbox"]:checked + label span {
    background:url(pictures/check1.png);
    background-size:contain;
}

input[type="radio"]{
    display:none;
}
input[type="radio"] + label span {
    display: inline-block;
    width: 4vh;
    height: 4vh;
    border-radius: 0vh;
    margin-right: 0.5vw;
    background:url(pictures/offradio.png);
    background-size:contain;
    cursor:pointer;
}
input[type="radio"]:checked + label span {
    background:url(pictures/onradio.png);
    background-size:contain;
}

#popup_form {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    right: 0;
    margin: 0;
    background-color:rgba(144, 142, 142, 0.69);

    .center_popup_form {
        width: 400px;
        margin: auto;
        position: relative;
        top: 50%;
        transform: translate(0%, -50%);
        .titlePage {
            height: 300px;
            margin: 0;
            background-color: #FF714C; 
            width: 100%;
            border-radius:10px;
            
            a#close_popup {
                position: absolute;
                right: 0;
                top: 0;
                padding: 5px;
                border-top-right-radius: 10px;
                display: block;
                background-color: #36b66e;
                color:white;
                cursor:pointer;
            }
                
            .important_popup {
                height: 300px;
                display: table-cell;
                text-align: center;
                vertical-align: middle;
                
                p.description-text {
                    font-size:20px;
                    textarea {
                        resize:none;
                        width:100%;
                    }
                }
                
                button {
                    width:50%;
                }
                
                a {
                    display:block;
                }
            }
        }
    }
}

#popup_form.disabled {
    display:none;
    visibility:hidden;
}

