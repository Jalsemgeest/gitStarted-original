.login-form, .register-form {
    label {
        color:white;
        text-align: left;
        width: 350px;
        display: block;
        margin: auto;
    }
    input {
        display:block;
        margin:10px auto;
        width: 350px;
        outline:none;
        background-color:white;
        color:black;
        border:none;
        height:30px;
        font-size:20px;
        text-indent:5px;
        box-shadow:2px 4px 3px 0 rgba(0,0,0,0.62);
    }
    input:focus, input:active {
        outline:none;
        background-color:transparent;
    }
    button {
        width:350px;
        border:0px;
        background-color:awesomeGreen;
        height:40px;
        color:white;
        font-size:18px;
        margin-top:5px;
        box-shadow:2px 4px 6px 0px midAlphaBlack;
    }
    button:hover, button:focus, button:active {
        cursor:pointer;
        -webkit-animation: bouncing-button 1s infinite; /* Safari 4+ */
        -moz-animation:    bouncing-button 1s infinite; /* Fx 5+ */
        -o-animation:      bouncing-button 1s infinite; /* Opera 12+ */
        animation:         bouncing-button 1s infinite; /* IE 10+, Fx 29+ */
    }
}

