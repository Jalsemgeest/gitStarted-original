/* Search Result */
.search_result_list_item {
    display:inline-block;
    width:33.33333%;
    // margin:1%;
    float:left;
    height:75px;
    // background-color:#CCCCCC;
    // border:1px solid #000000;
    // box-sizing:border-box;
        
    position:relative;
    text-align:center;
    margin:auto;
    margin-top:10px;
    
    label {
        border-radius: 10px;
        border-width: 3px;
        border-color: rgba(255,255,255,1);
        background-color:rgba(255,255,255,0.8);
        width:98%
        height:100%;
        display:block;
    }
}

#search_module_results {
    ul {
        padding: 0;
        list-style: none;
        margin-top: 50px;
        display: block;
        text-align: center;
        margin: 50px auto;
        
        li.search_result_list_item {
            float:none;
            width:33%;
            
            label {
                width:100%
                border-radius:10px;
                border:3px solid white;
                box-sizing:border-box;
            }
            
            label:hover {
                background-color: rgba(255,255,255,0.2) !important;
                cursor:pointer;
                border-radius:10px;
                border:3px solid white;
                box-sizing:border-box;
            }
            
            input[type=checkbox] {
                position:relative;
                left:-100000px;
            }
            
            label.added-module, label.added-module:hover {
                background-color:white !important;
                // border-radius: 5vh;
                border-color:black;
            }
        }
    }
}

input#check_jquery, input#check_bootstrap, input#check_fontawesome {
    position:relative;
    left:-10000px;
}
    

