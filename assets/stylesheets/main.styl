.sign-up-login {
    text-decoration:none;
    display:block;
    color:white;
    font-size:20px;
    font-family:'Myriad Pro';
    background-color:awesomeGreen;
    width: 200px;
    height: 60px;
    line-height: 60px;
    margin: auto;
    border-radius:3px;
    box-shadow:2px 4px 2px 0px midAlphaBlack;
}

.sign-up-login:hover, .sign-up-login:focus, .sign-up-login:active {
    -webkit-animation: bouncing-button 1s infinite; /* Safari 4+ */
    -moz-animation:    bouncing-button 1s infinite; /* Fx 5+ */
    -o-animation:      bouncing-button 1s infinite; /* Opera 12+ */
    animation:         bouncing-button 1s infinite; /* IE 10+, Fx 29+ */
}

@keyframes bouncing-button {
    0% {
        transform:translate(0px, 0px) scale(1);
        box-shadow:2px 4px 2px 0px midAlphaBlack;
    }
    25% {
        transform:translate(0px, -1px) scale(1.005);
        box-shadow:2px 4px 5px 1px midAlphaBlack;
    }
    50% {
        transform:translate(0px, -2px) scale(1.01);
        box-shadow:2px 4px 8px 1px midAlphaBlack;
    }
    75% {
        transform:translate(0px, -1px) scale(1.005);
        box-shadow:2px 4px 5px 1px midAlphaBlack;
    }
    100% {
        transform:translate(0px, 0px) scale(1);
        box-shadow:2px 4px 2px 0px midAlphaBlack;
    }
}

