    }
}
.user_image {
    position:absolute;
    width:80px;
    height:80px;
    top:10px;
    right:10px;
    float:left;
    a {
        display:block;
        width:80px;
        height:80px;
        border-radius:100px;
        img {
            height:80px;
            width:80px;
            border-radius:100px;
        }
    }
    a:hover, a:focus, a:active {
        
    }
}

