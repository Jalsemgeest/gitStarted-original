}
.submitButton{
    margin: 10vh 0vh 10vh 0vh;
    background-color: rgba(255,255,255,0);
    border-radius: 5vh !important;
    border-width: 1vh;
    border-color: rgba(255,255,255,1);
    padding-left: 1vh;
    height: 10vh;
    width: 30vw;
    font-family: 'Oswald';
    font-size:5vh;
    color: white;
    display: block;
    
}
button.submitButton.smallSubmit {
    margin:10px 0 15px 0;
    height:40px;
    font-size:20px;
    border-radius:10px;
    border-width:3px;
    
}

#submit_image_button:hover {
    opacity: 0.7;
}

