        text-align: center;
        color: white;
        font-family: 'Oswald';
        font-weight: 100;
    }
}

.loginTitle {
    margin-top: 8vh;
    font-family: 'Oswald';
    font-size: 8vh;
    color: white;
}

.password {
    display: list-item;
    margin: 5vh;
    background-color: rgba(255,255,255,0.8);
    border-radius: 3vh;
    border-width: 1vh;
    border-color: #FFAC4C;
    padding-left: 1vh;
    height: 4vh;
    width: 30vw;
}

.username {
    display: list-item;
    margin-top: 5vh;
    background-color: rgba(255,255,255,0.8);
    border-radius: 3vh;
    border-width: 1vh;
